{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "meta-viewport": "off",
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-color",
          "scrollbar-width",
          "width: fit-content"
        ]
      }
    ],
    "axe/structure": "off",
    "no-inline-styles": "off"
  }
}